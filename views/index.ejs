  <body>
      <section class="form-section">

          <div class="row">
              <div class="col-md-6" id="waves">
                  <img class="wave" src="assets/img/wave.png">
                  <div class="img">
                      <img src="assets/img/undraw_file_searching_duff.svg">
                  </div>
              </div>
              <div class="col-md-6" id="main-content">
                  <img class="mobile" src="assets/img/undraw_file_searching_duff.svg">
                  <h2>URL SHORTENER</h2>
                  <form class="url-form">
                      <input class="url-input" type="url" placeholder="Enter or Paste A Link" name="url" autocomplete="off" autocorrect="off" autocapitalize="off" spellcheck="false" required><br>
                      <button type="submit" class="submit-button">Shorten!</button>
                  </form>
                  <div class="results">
                      <h3>Shortened Links</h3>
                      <div class="result-section">

                      </div>
                  </div>
